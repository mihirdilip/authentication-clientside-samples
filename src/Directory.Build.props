<Project>

	<PropertyGroup>
		<OutputType>Exe</OutputType>
		<TargetFramework>netcoreapp3.1</TargetFramework>
		<RootNamespace>Authentication.ClientSide</RootNamespace>
		<Configurations>Debug;Release;Local</Configurations>
		<!--<Configuration Condition="'$(MSBuildProjectExtension)' == '.csproj'">Debug;Release;Local</Configuration>-->
	</PropertyGroup>

	<PropertyGroup>
		<Version>2.2.0-preview.5</Version>
		<Copyright>Copyright (c) 2020 Mihir Dilip</Copyright>
		<Authors>Mihir Dilip</Authors>
		<Company>Mihir Dilip</Company>
		<PackageProjectUrl>https://github.com/mihirdilip/authentication-clientside-samples</PackageProjectUrl>
		<RepositoryUrl>https://github.com/mihirdilip/authentication-clientside-samples</RepositoryUrl>
		<RepositoryType>GitHub</RepositoryType>
		<PackageLicenseFile>LICENSE</PackageLicenseFile>
		<PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
	</PropertyGroup>

	<ItemGroup>
		<None Include="..\..\LICENSE">
			<Pack>True</Pack>
			<PackagePath></PackagePath>
		</None>
	</ItemGroup>

	<PropertyGroup Condition="'$(Configuration)'=='Release'">
		<GeneratePackageOnBuild>true</GeneratePackageOnBuild>
	</PropertyGroup>

</Project>